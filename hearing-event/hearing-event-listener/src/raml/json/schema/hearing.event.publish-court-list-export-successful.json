{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/listing/event/hearing.event.publish-court-list-export-successful.json",
  "type": "object",
  "properties": {
    "courtCentreId": {
      "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/uuid"
    },
    "courtListFileName": {
      "type": "string"
    },
    "publishStatus": {
      "type": "string",
      "enum": [
        "EXPORT_FAILED",
        "EXPORT_SUCCESSFUL",
        "COURT_LIST_REQUESTED"
      ]
    },
    "createdTime": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "courtCentreId",
    "courtListFileName",
    "publishStatus",
    "createdTime"
  ],
  "additionalProperties": false
}