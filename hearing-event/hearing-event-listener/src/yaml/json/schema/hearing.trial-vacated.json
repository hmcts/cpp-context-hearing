{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/hearing/courts/hearing.trial-vacated.json",
  "type": "object",
  "properties": {
    "hearingId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "vacatedTrialReasonId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "code": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "courtCentreId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "hasInterpreter": {
      "type": "boolean"
    },
    "caseDetails": {
      "type": "array",
      "minItems": 0,
      "items": {
        "$ref": "http://justice.gov.uk/hearing/courts/hearing.case-details.json"
      }
    },
    "applicationDetails": {
      "type": "array",
      "minItems": 0,
      "items": {
        "$ref": "http://justice.gov.uk/hearing/courts/hearing.application-details.json"
      }
    },
    "jurisdictionType": {
      "$ref": "http://justice.gov.uk/core/courts/jurisdictionType.json"
    },
    "hearingDay": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "hearingId",
    "vacatedTrialReasonId",
    "code",
    "type",
    "description"
  ],
  "additionalProperties": false
}