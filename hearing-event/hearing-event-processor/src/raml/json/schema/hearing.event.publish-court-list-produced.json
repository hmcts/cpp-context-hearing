{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/listing/event/hearing.event.publish-court-list-produced.json",
  "type": "object",
  "properties": {
    "courtCentreId": {
      "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/uuid"
    },
    "courtListFileId": {
      "$ref": "http://justice.gov.uk/core/courts/courtsDefinitions.json#/definitions/uuid"
    },
    "courtListFileName": {
      "type": "string"
    },
    "publishCourtListType": {
      "type": "string",
      "enum": [
        "WARN",
        "DRAFT",
        "FIRM",
        "FINAL"
      ]
    },
    "publishStatus": {
      "type": "string",
      "enum": [
        "EXPORT_FAILED",
        "EXPORT_SUCCESSFUL",
        "COURT_LIST_REQUESTED",
        "COURT_LIST_PRODUCED"
      ]
    },
    "createdTime": {
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": false,
  "required": [
    "courtCentreId",
    "courtListFileId",
    "courtListFileName",
    "publishCourtListType",
    "publishStatus",
    "createdTime"
  ]
}