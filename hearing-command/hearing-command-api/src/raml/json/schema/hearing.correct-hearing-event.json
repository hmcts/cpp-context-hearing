{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/hearing/hearing.correct-hearing-event.json",
  "type": "object",
  "properties": {
    "latestHearingEventId": {
      "$ref": "http://justice.gov.uk/json/schemas/core/courtsDefinitions.json#/definitions/uuid"
    },
    "hearingEventDefinitionId": {
      "$ref": "http://justice.gov.uk/json/schemas/core/courtsDefinitions.json#/definitions/uuid"
    },
    "recordedLabel": {
      "type": "string"
    },
    "eventTime": {
      "type": "string",
      "format": "date-time"
    },
    "lastModifiedTime": {
      "type": "string",
      "format": "date-time"
    },
    "counselId": {
      "$ref": "http://justice.gov.uk/json/schemas/core/courtsDefinitions.json#/definitions/uuid"
    }
  },
  "additionalProperties": false,
  "required": [
    "latestHearingEventId",
    "recordedLabel",
    "eventTime",
    "lastModifiedTime"
  ]
}