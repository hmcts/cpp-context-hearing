{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/hearing/hearing.log-hearing-event.json",
  "type":"object",
  "properties":{
    "hearingEventId":{
      "$ref":"http://justice.gov.uk/json/schemas/core/courtsDefinitions.json#/definitions/uuid"
    },
    "hearingId":{
      "$ref":"http://justice.gov.uk/json/schemas/core/courtsDefinitions.json#/definitions/uuid"
    },
    "hearingEventDefinitionId":{
      "$ref":"http://justice.gov.uk/json/schemas/core/courtsDefinitions.json#/definitions/uuid"
    },
    "defenceCounselId":{
      "$ref":"http://justice.gov.uk/json/schemas/core/courtsDefinitions.json#/definitions/uuid"
    },
    "recordedLabel":{
      "type":"string"
    },
    "eventTime":{
      "type":"string",
      "format":"date-time"
    },
    "lastModifiedTime":{
      "type":"string",
      "format":"date-time"
    },
    "alterable":{
      "type":"boolean"
    }
  },
  "required":[
    "hearingEventId",
    "hearingEventDefinitionId",
    "recordedLabel",
    "eventTime",
    "lastModifiedTime"
  ],
  "additionalProperties":false
}
