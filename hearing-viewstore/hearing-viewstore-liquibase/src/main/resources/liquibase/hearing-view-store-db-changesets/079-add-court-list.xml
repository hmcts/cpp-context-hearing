<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="8" author="TechPod" logicalFilePath="079-add-court-list.xml">
        <createTable tableName="court_list_publish_status">
            <column name="court_list_publish_status_id" type="UUID">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="court_centre_id" type="UUID">
                <constraints  nullable="false"/>
            </column>
            <column name="publish_status" type="TEXT">
                <constraints nullable="false"/>
            </column>
            <column name="last_updated" type="DATETIME">
                <constraints nullable="false"/>
            </column>
            <column name="court_list_file_id" type="UUID"/>
            <column name="court_list_file_name" type="TEXT"/>
            <column name="error_message" type="TEXT"/>
        </createTable>
    </changeSet>
</databaseChangeLog>