<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet author="CCR" id="1" logicalFilePath="003-create-hearing-event-definition.xml">
        <createTable tableName="hearing_event_definitions">
            <column name="recorded_label" type="VARCHAR(200)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="action_label" type="VARCHAR(200)">
                <constraints nullable="false"/>
            </column>
            <column name="sequence_number" type="INT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>


</databaseChangeLog>